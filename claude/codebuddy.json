{
    "mcpServers": {
        "sequential-thinking": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-sequential-thinking"
            ]
        },
        "context7": {
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp@latest"
            ]
        },
        "mcp-feedback-enhanced": {
            "command": "uvx",
            "args": [
                "mcp-feedback-enhanced@latest"
            ],
            "timeout": 600,
            "env": {
                "MCP_DESKTOP_MODE": "true",
                "MCP_WEB_PORT": "8765",
                "MCP_DEBUG": "false"
            },
            "autoApprove": [
                "interactive_feedback"
            ]
        },
        "filesystem": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-filesystem",
                "/Users/edte/"
            ]
        },
        "time": {
            "command": "uvx",
            "args": [
                "mcp-server-time",
                "--local-timezone=America/New_York"
            ]
        },
        "amap-maps": {
            "command": "npx",
            "args": [
                "-y",
                "@amap/amap-maps-mcp-server"
            ],
            "env": {
                "AMAP_MAPS_API_KEY": "3f8a6fe1783549e95e7a49e145e563d8"
            }
        }
    }
}
